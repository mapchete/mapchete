process: geojson_test.py
zoom_levels: 4
pyramid:
    grid: geodetic
    metatiling: 4
input:
    file1:
        format: TileDirectory
        path: tmp/geojson
        metatiling: 2
        grid: geodetic
        extension: geojson
output:
    format: GeoJSON
    path: tmp/geojson_tiledir
    data_type: vector
    schema:
        properties:
            name: str
            id: int
            area: float
        geometry: Polygon
    metatiling: 2
