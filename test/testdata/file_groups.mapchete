process: ../example_process.py
zoom_levels:
    min: 0
    max: 5
pyramid:
    grid: geodetic
    pixelbuffer: 10
    metatiling: 8
input:
    group1:
        file1: cleantopo_tl.tif
        file2: cleantopo_br.tif
    group2:
        file1: dummy1.tif
        file2: dummy2.tif
    nested_group:
        group1:
            file1: cleantopo_tl.tif
            file2: cleantopo_br.tif
        group2:
            file1: dummy1.tif
            file2: dummy2.tif
output:
    dtype: uint16
    bands: 1
    format: GTiff
    path: tmp/file_groups
