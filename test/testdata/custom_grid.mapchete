process: ../example_process.py
zoom_levels: 5
pyramid:
    grid:
        shape: [1, 1]
        bounds: [-6422528.0, -6422528.0, 6422528.0, 6422528.0]
        is_global: False
        proj: "+proj=ortho +lat_0=-90 +lon_0=0 +x_0=0 +y_0=0 +ellps=WGS84 +units=m +no_defs"
    pixelbuffer: 20
    metatiling: 8
input:
    file1: cleantopo_br.tif
output:
    dtype: uint16
    bands: 1
    format: GTiff
    path: tmp/custom_grid
    pixelbuffer: 20
