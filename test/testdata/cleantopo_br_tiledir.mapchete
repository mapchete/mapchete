process: ../example_process.py
zoom_levels:
    min: 0
    max: 5
pyramid:
    grid: geodetic
    pixelbuffer: 20
    metatiling: 4
input:
    file1:
        format: TileDirectory
        path: tmp/cleantopo_br
        metatiling: 8
        type: geodetic
        extension: tif
        dtype: uint16
        count: 1

output:
    dtype: uint16
    bands: 1
    format: GTiff
    path: tmp/cleantopo_br_tiledir
